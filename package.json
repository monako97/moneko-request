{
  "name": "@moneko/request",
  "version": "1.5.3",
  "description": "A lightweight and flexible request library for Node.js and browsers",
  "type": "module",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "browser": "./esm/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./esm/node.js",
        "require": "./cjs/node.js"
      },
      "browser": {
        "import": "./esm/index.js",
        "require": "./cjs/index.js"
      },
      "default": {
        "import": "./esm/index.js",
        "require": "./cjs/index.js"
      }
    }
  },
  "scripts": {
    "prepack": "node ./build.mjs",
    "build": "node ./build.mjs"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monako97/moneko-request.git"
  },
  "bugs": {
    "url": "https://github.com/monako97/moneko-request/issues"
  },
  "engines": {
    "node": ">=14.16"
  },
  "keywords": [
    "http",
    "request",
    "fetch",
    "nodejs",
    "browser"
  ],
  "author": "moneko <poi.nyaa@qq.com>",
  "license": "MIT",
  "files": [
    "esm",
    "cjs"
  ],
  "devDependencies": {
    "@moneko/convert": "1.0.1",
    "@types/node": "22.14.0",
    "@moneko/eslint": "1.3.0"
  }
}
